{
  "name": "craftcms/craft",
  "description": "Craft CMS",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/cms/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/craftcms/cms",
    "docs": "https://craftcms.com/docs",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "php": ">=7.0.0",
    "craftcms/cms": "3.1.19",
    "vlucas/phpdotenv": "v2.4.0",
    "roave/security-advisories": "dev-master#3df94834c80037130b533703df4672785b6ea112",
    "ether/simplemap": "3.3.1",
    "craftcms/element-api": "2.5.2",
    "nystudio107/craft-imageoptimize": "1.4.16",
    "craftcms/simple-text": "2.0.2",
    "craftcms/redactor": "1.0.1",
    "dukt/videos": "2.0.0-beta.6",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "mikestecker/craft-videoembedder": "1.0.4",
    "rias/craft-colour-swatches": "1.1.0",
    "dolphiq/sitemap": "1.0.9",
    "bower-asset/bootstrap": "v3.3.5",
    "bower-asset/inputmask": "3.3.11",
    "bower-asset/jquery": "3.2.1",
    "bower-asset/punycode": "v1.3.2",
    "bower-asset/yii2-pjax": "2.0.7.1",
    "cloudinary/cloudinary_php": "1.8.0",
    "dukt/oauth2-google": "2.0.0",
    "dukt/oauth2-vimeo": "2.0.0",
    "imageoptim/imageoptim": "1.3.1",
    "imgix/imgix-php": "1.2.1",
    "jaybizzle/crawler-detect": "v1.2.62",
    "kraken-io/kraken-php": "1.6",
    "ksubileau/color-thief-php": "v1.3.1",
    "league/fractal": "0.16.0",
    "league/oauth2-google": "2.0.1",
    "markhuot/craftql": "1.3.1",
    "verbb/super-table": "2.1.8"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ]
  }
}
